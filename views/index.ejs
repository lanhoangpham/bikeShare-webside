<html>

<head>  
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <link rel="icon" type="image/x-icon" href="./assset/img/img-buyTicket.webp">

    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>bikeShare</title>
    <link rel="stylesheet" href="./assset/style/style.css">
    <link rel="stylesheet" href="./assset/font/themify-icons/themify-icons.css" class="">
</head>

<body>

    <div class="main">

        <header class="header">

            <div class="header_title">
                BIKESHARE
                <div class="menu_in_header" onclick="changeSidebar()">
                    <i class="ti-menu-alt"></i>
                </div>
            </div>

            <div class="header_forUser">
                <button onclick="logoutFunc()">Đăng xuất</button>
                <!-- <a href="" class="user"><i class="ti-user"></i></a> -->
            </div>

        </header>
        

        <!--begin content-->
        <div class="content">

            <div class="sidebar open">
                
               <div class="sidebar_user">
                  <div class="boder">
                        <svg  viewBox="0 0 80 80" width="80" height="80">
                            <circle class="circle" cx="40" cy="40" r="38"/>
                            
                        </svg>
                        <p class="UserName">UserName</p>
                    </div>
                    
               </div>
               
               <div class="sidebar_option_list">
                <div class="sidebar_option js_homePage"><a href="./homepage">Trang chủ</a></div>
                <div class="sidebar_option js_infor"><a href="./info">Thông tin cá nhân</a></div>
                <div class="sidebar_option js_myWallet"><a href="./wallet">Ví của tôi</a></div>
                <div class="sidebar_option js_buyTicket"><a href="./ticket">Mua vé</a></div>
                <div class="sidebar_option js_history"><a href="./history">Chuyến đi của tôi</a></div>
                <div class="sidebar_option js_instruction"><a href="./instruction">Hướng dẫn sử dụng</a></div>
             </div>
                
            </div>
            
            <!--begin homePage-->

            <div class="content_common honePage">
                <div class="homePage_border">
                    <div id="map">  
                        
                    </div>

                    <script async
                        src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAtdTWAUA78Q0bf5FaWF1o5ABbIvWkjXCE&callback=initMap">
                        defer
                    </script>

                    <div class="homePage_search">
                        <input type="text" class="homepage_inputSearch" placeholder="Tìm trạm xe">
                        <div class="homePage_iconSerach">
                            <i class="ti-search"></i>
                        </div>
                    </div>

                </div>
                
                   
            </div>
            <!--end homePage-->

        </div>
    </div>

    <script src="./js/main.js"></script>

</body>
</html>